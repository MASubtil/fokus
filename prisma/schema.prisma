datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Item {
  id        Int      @id @default(autoincrement())
  name      String
  createdAt DateTime @default(now())
}

model FocusSession {
  id        Int      @id @default(autoincrement())
  startedAt DateTime @default(now())
  duration  Int
  note      String?
}